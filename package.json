{
  "name": "alpha-observatory-management-system",
  "version": "0.1.1",
  "private": false,
  "description": "Alpha Observatory Management System",
  "author": {
    "name": "Filipe Mota",
    "github": "https://github.com/Drete457"
  },
  "copyright": "Copyright 2020 - 2022 CEO NADA",
  "repository": {
    "type": "git",
    "url": "https://github.com/Drete457/alphaobservatorymanagementsystem.git"
  },
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "set \"CI=false\" && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "upgrade": "yarn upgrade-interactive"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb/hooks",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "react"
    ],
    "rules": {
      "react/jsx-uses-react": 2,
      "react/react-in-jsx-scope": "off",
      "sass/quietDeps": "off"
    },
    "overrides": [
      {
        "files": [
          "**/*.js?(x)"
        ],
        "rules": {
          "screen": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 10",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "type": "module",
  "dependencies": {
    "@coreui/coreui-pro": "^3.4.2",
    "@coreui/icons": "^2.1.0",
    "@coreui/icons-react": "^1.1.0",
    "@coreui/react": "^3.4.6",
    "@coreui/utils": "^1.3.1",
    "ag-grid-community": "^26.2.1",
    "ag-grid-enterprise": "^26.2.1",
    "ag-grid-react": "^26.2.0",
    "export-from-json": "^1.4.1",
    "firebase": "^9.4.0",
    "i18next": "^21.4.2",
    "moment-timezone": "^0.5.35",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-grid-layout": "^1.3.0",
    "react-i18next": "^11.14.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-select": "^5.2.1",
    "read-excel-file": "^5.2.21",
    "recoil": "^0.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "babel-core": "^6.26.3",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.1",
    "sass": "^1.49.7",
    "yarn-upgrade-all": "^0.5.4"
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9"
  }
}
